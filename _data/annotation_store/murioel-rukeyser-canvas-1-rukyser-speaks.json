      "motivation": [
        "supplementing",
        "commenting"
      ],
      "body": {
        "type": "TextualBody",
        "value": "Besides, very few people do it. And it's all lies, you know.",
        "format": "text/plain",
        "purpose": "commenting"
      },
      "target": {
        "source": {
          "id": "https://spampell.github.io/mrw/murioel-rukeyser/canvas-1/canvas",
          "type": "Canvas",
          "partOf": [
